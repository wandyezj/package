    "dependencies": {},
    "devDependencies": {
        "typescript": "^4.1.3",
        "prettier": "^2.2.1",
        "jest": "^26.6.3",
        "ts-jest": "^26.4.4",
        "eslint": "^7.18.0",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0"
    }
